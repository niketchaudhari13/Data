create or replace procedure cnt_emp(dno in number,cnt out number)
as
begin
select count(*) into cnt from emp where deptno=dno;
dbms_output.put_line('count:'||cnt);
end;

----------to execute---------------------
variable dcnt number
execute cnt_emp(10,:cnt)
print dcnt;


